<Window x:Class="Initiative_tracker.ChangeHealthDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Initiative_tracker"
        mc:Ignorable="d"
        Title="ChangeHealthDialog" Height="300" Width="300">
    <Grid>
        <DockPanel>
            <DockPanel>
                <TextBlock Text="Name: "><Run Text="{Binding name}"></Run></TextBlock>
            </DockPanel>
            <DockPanel>
                <TextBlock Text="Health: "/>
                <TextBox Text="{Binding health}"></TextBox>
            </DockPanel>
        </DockPanel>
    </Grid>
</Window>
